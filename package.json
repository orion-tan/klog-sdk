{
  "name": "@klog/sdk",
  "version": "1.0.0",
  "description": "KLog API TypeScript SDK",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": ["dist"],
  "repository": {
    "type": "git",
    "url": "https://github.com/twiify/klog-sdk.git"
  },
  "homepage": "https://github.com/twiify/klog-sdk#readme",
  "bugs": {
    "url": "https://github.com/twiify/klog-sdk/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "lint": "eslint src --ext .ts",
    "test": "vitest",
    "prepublishOnly": "npm run build",
    "postversion": "git push && git push --tags"
  },
  "dependencies": {
    "axios": "^1.6.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0",
    "tsup": "^8.0.0",
    "eslint": "^8.0.0",
    "vitest": "^1.0.0"
  },
  "keywords": ["klog", "blog", "api", "sdk", "typescript"],
  "author": "twiify",
  "license": "MIT"
}

